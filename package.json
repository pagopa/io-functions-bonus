{
  "name": "io-functions-bonus",
  "description": "",
  "author": "IO team",
  "repository": "https://github.com/pagopa/io-functions-bonus",
  "version": "1.18.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "shx rm -rf dist && yarn generate",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "func extensions install --javascript",
    "start": "dotenv -e .env func start --javascript",
    "predev": "npm-run-all generate build",
    "dev": "npm-run-all --parallel start watch",
    "pretest": "npm-run-all generate:*",
    "test": "yarn test:only",
    "test:only": "jest",
    "pretest:coverage": "npm-run-all generate:*",
    "test:coverage": "jest --coverage",
    "lint": "tslint -p .",
    "lint-api": "oval validate -p openapi/index.yaml",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "dist:zip": "zip -r dist.zip . --exclude @.funcignore --exclude .funcignore",
    "predeploy": "npm-run-all generate build dist:modules",
    "generate:definitions": "shx rm -rf ./generated/definitions && shx mkdir -p ./generated/definitions && gen-api-models --request-types --api-spec ./openapi/index.yaml --out-dir ./generated/definitions",
    "generate:models": "shx rm -rf ./generated/models && shx mkdir -p ./generated/models && gen-api-models --strict 0 --camel-cased --api-spec ./openapi/index.yaml --out-dir ./generated/models",
    "generate:ade": "shx rm -rf ./generated/ade && shx mkdir -p ./generated/ade && gen-api-models --request-types --response-decoders --camel-cased --api-spec ./openapi/ade.yaml --out-dir ./generated/ade",
    "generate": "npm-run-all generate:*"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/bluebird": "^3.5.32",
    "@types/documentdb": "^1.10.5",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.15",
    "@types/node": "10.14.1",
    "@types/vfile-message": "^2.0.0",
    "@types/xmldom": "^0.1.29",
    "apiconnect-wsdl": "^1.8.21",
    "danger": "^4.0.2",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "dotenv-cli": "^3.1.0",
    "fast-check": "^1.25.1",
    "italia-tslint-rules": "*",
    "italia-utils": "^4.3.0",
    "jest": "^24.8.0",
    "jest-mock-express": "^0.1.1",
    "js-yaml": "^3.14.0",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "oval": "^1.0.0",
    "prettier": "^1.18.2",
    "release-it": "^12.4.3",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.9.0"
  },
  "dependencies": {
    "applicationinsights": "^1.7.5",
    "azure-storage": "^2.10.3",
    "date-fns": "^2.14.0",
    "documentdb": "^1.15.3",
    "durable-functions": "^1.4.1",
    "express": "^4.15.3",
    "fp-ts": "1.17.4",
    "io-functions-commons": "^10.5.0",
    "io-functions-express": "^0.1.0",
    "io-ts": "1.8.5",
    "italia-ts-commons": "^8.5.0",
    "node-fetch": "^2.6.0",
    "winston": "^3.2.1",
    "xmldom": "^0.3.0"
  },
  "resolutions": {
    "fp-ts": "1.17.4",
    "io-ts": "1.8.5"
  }
}
