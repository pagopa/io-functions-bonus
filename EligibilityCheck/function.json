{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "api/v1/bonus/vacanze/eligibility/{fiscalcode}",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "starter",
      "type": "durableClient",
      "direction": "in"
    },
    {
      "type": "cosmosDB",
      "name": "processingBonusIdIn",
      "databaseName": "%COSMOSDB_BONUS_DATABASE_NAME%",
      "collectionName": "bonus-processing",
      "connectionStringSetting": "COSMOSDB_CONNECTION_STRING",
      "direction": "in",
      "id": "{fiscalcode}",
      "partitionKey": "{fiscalcode}"
    },
    {
      "type": "cosmosDB",
      "name": "eligibilityCheck",
      "databaseName": "%COSMOSDB_BONUS_DATABASE_NAME%",
      "collectionName": "eligibility-checks",
      "connectionStringSetting": "COSMOSDB_CONNECTION_STRING",
      "direction": "in",
      "id": "{fiscalcode}",
      "partitionKey": "{fiscalcode}"
    }
  ],
  "scriptFile": "../dist/EligibilityCheck/index.js"
}
